{
  "name": "Enhanced Google Sheets to Backend Sync with Drive Integration",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "sync-from-sheets",
        "options": {}
      },
      "id": "webhook-trigger",
      "name": "Webhook Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [240, 300],
      "webhookId": "sync-from-sheets-webhook"
    },
    {
      "parameters": {
        "authentication": "oAuth2",
        "resource": "spreadsheet",
        "operation": "read",
        "documentId": "YOUR_GOOGLE_SHEET_ID",
        "sheetName": "Inventario a web",
        "options": {
          "range": "A2:I1000"
        }
      },
      "id": "read-google-sheets",
      "name": "Read Google Sheets",
      "type": "n8n-nodes-base.googleSheets",
      "typeVersion": 4.1,
      "position": [460, 300]
    },
    {
      "parameters": {
        "jsCode": "// Process Google Sheets data and convert to backend format with Drive integration\nconst items = $input.all();\nconst processedItems = [];\n\nfor (const item of items) {\n  const row = item.json;\n  \n  // Skip empty rows\n  if (!row['Modelo'] || !row['Marca']) continue;\n  \n  // Convert Google Sheets format to backend format\n  const vehicle = {\n    marca: String(row['Marca'] || ''),\n    modelo: String(row['Modelo'] || ''),\n    año: (() => {\n      const año = parseInt(row['Año']);\n      return isNaN(año) ? 2000 : año; // Default to 2000 if invalid\n    })(),\n    color: String(row['Color'] || ''),\n    precio: (() => {\n      const precio = row['Precio'];\n      if (!precio || precio === 'INFO' || precio === '') return 0;\n      if (typeof precio === 'string') {\n        return parseFloat(precio.replace(/[$,]/g, '')) || 0;\n      }\n      return parseFloat(precio) || 0;\n    })(),\n    kilometraje: String(row['km'] || ''),\n    estatus: String((row['Estatus'] || 'DISPONIBLE').toUpperCase()),\n    ubicacion: String(row['Ubicacion'] || ''),\n    descripcion: String(`${row['Marca']} ${row['Modelo']} ${row['Año']}`),\n    external_id: String(`GS_${row['Modelo']}_${row['Año']}_${Math.random().toString(36).substr(2, 9)}`),\n    // Drive integration fields\n    create_drive_folder: true,\n    drive_folder_name: `${row['Marca']}_${row['Modelo']}_${row['Año']}_${row['Precio']}_${new Date().toISOString().split('T')[0]}`\n  };\n  \n  processedItems.push({ json: vehicle });\n}\n\nreturn processedItems;"
      },
      "id": "process-data",
      "name": "Process Data",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [680, 300]
    },
    {
      "parameters": {
        "url": "http://127.0.0.1:8000/vehicles/sync-from-sheets",
        "authentication": "none",
        "requestMethod": "POST",
        "sendBody": true,
        "bodyContentType": "json",
        "specifyBody": "json",
        "jsonBody": "={{ $input.all().map(item => item.json) }}",
        "options": {}
      },
      "id": "sync-to-backend",
      "name": "Sync to Backend",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [900, 300]
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict"
          },
          "conditions": [
            {
              "id": "check-new-vehicle",
              "leftValue": "={{ $json.status }}",
              "rightValue": "created",
              "operator": {
                "type": "string",
                "operation": "equals"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "check-new-vehicle",
      "name": "Check New Vehicle",
      "type": "n8n-nodes-base.if",
      "typeVersion": 2,
      "position": [1120, 300]
    },
    {
      "parameters": {
        "authentication": "oAuth2",
        "resource": "drive",
        "operation": "create",
        "name": "={{ $json.drive_folder_name }}",
