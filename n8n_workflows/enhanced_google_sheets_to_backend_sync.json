        "bodyContentType": "json",
        "specifyBody": "json",
        "jsonBody": "={\n  \"drive_folder_id\": \"{{ $json.id }}\",\n  \"drive_folder_url\": \"{{ $json.webViewLink }}\"\n}",
        "options": {}
      },
      "id": "update-vehicle-drive",
