{
  "name": "autosell-backend",
  "version": "1.0.0",
  "description": "Autosell.mx Backend API",
  "main": "main.py",
  "scripts": {
    "dev": "uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "start": "uvicorn main:app --host 0.0.0.0 --port 8000",
    "build": "echo 'Python backend - no build step required'",
    "test": "pytest",
    "test:coverage": "pytest --cov=app --cov-report=html",
    "test:watch": "pytest --watch",
    "lint": "flake8 app tests",
    "format": "black app tests",
    "format:check": "black --check app tests",
    "sort": "isort app tests",
    "sort:check": "isort --check-only app tests",
    "type-check": "mypy app",
    "db:migrate": "alembic upgrade head",
    "db:rollback": "alembic downgrade -1",
    "db:revision": "alembic revision --autogenerate -m",
    "db:seed": "python scripts/seed_database.py",
    "db:reset": "python scripts/reset_database.py",
    "logs": "tail -f logs/autosell.log",
    "install": "pip install -r requirements.txt",
    "install:dev": "pip install -r requirements.txt && pip install -r requirements-dev.txt",
    "clean": "find . -type f -name '*.pyc' -delete && find . -type d -name '__pycache__' -delete",
    "docker:build": "docker build -t autosell-backend .",
    "docker:run": "docker run -p 8000:8000 autosell-backend"
  },
  "keywords": [
    "fastapi",
    "python",
    "vehicle-management",
    "api"
  ],
  "author": "Autosell.mx Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
